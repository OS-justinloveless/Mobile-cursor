{
  "name": "Rust",
  "scopeName": "source.rust",
  "fileTypes": ["rs"],
  "patterns": [
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#keywords" },
    { "include": "#types" },
    { "include": "#macros" },
    { "include": "#functions" },
    { "include": "#numbers" },
    { "include": "#operators" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.rust",
          "match": "//.*$"
        },
        {
          "name": "comment.block.rust",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.rust",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.rust",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.raw.rust",
          "begin": "r#*\"",
          "end": "\"#*"
        },
        {
          "name": "string.quoted.byte.rust",
          "begin": "b\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.rust",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.char.rust",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.rust",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.rust",
          "match": "\\b(if|else|match|loop|while|for|break|continue|return|yield|in)\\b"
        },
        {
          "name": "keyword.declaration.rust",
          "match": "\\b(fn|let|mut|const|static|type|struct|enum|trait|impl|mod|pub|use|crate|super|self|Self|as|where|unsafe|extern|async|await|dyn|move|ref)\\b"
        },
        {
          "name": "constant.language.rust",
          "match": "\\b(true|false|None|Some|Ok|Err)\\b"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "name": "support.type.primitive.rust",
          "match": "\\b(bool|char|str|u8|u16|u32|u64|u128|usize|i8|i16|i32|i64|i128|isize|f32|f64)\\b"
        },
        {
          "name": "support.type.rust",
          "match": "\\b(String|Vec|Box|Rc|Arc|Cell|RefCell|Option|Result|HashMap|HashSet|BTreeMap|BTreeSet|VecDeque|LinkedList|BinaryHeap|Cow|Pin|PhantomData|MaybeUninit)\\b"
        },
        {
          "name": "entity.name.type.rust",
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
        },
        {
          "name": "entity.name.type.lifetime.rust",
          "match": "'[a-z_][a-zA-Z0-9_]*"
        }
      ]
    },
    "macros": {
      "patterns": [
        {
          "name": "entity.name.function.macro.rust",
          "match": "\\b[a-z_][a-zA-Z0-9_]*!"
        },
        {
          "name": "entity.name.function.attribute.rust",
          "match": "#\\[.*?\\]"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "entity.name.function.rust",
          "match": "\\b([a-z_][a-zA-Z0-9_]*)\\s*(?=\\()"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.rust",
          "match": "\\b(0x[0-9a-fA-F_]+|0o[0-7_]+|0b[01_]+|[0-9][0-9_]*(\\.[0-9_]+)?([eE][+-]?[0-9_]+)?)(u8|u16|u32|u64|u128|usize|i8|i16|i32|i64|i128|isize|f32|f64)?\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.rust",
          "match": "[+\\-*/%=<>!&|^~?:]+|=>|->|\\.\\.|\\.\\.="
        }
      ]
    }
  }
}
