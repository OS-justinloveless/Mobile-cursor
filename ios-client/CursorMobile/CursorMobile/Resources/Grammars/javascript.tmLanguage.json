{
  "name": "JavaScript",
  "scopeName": "source.js",
  "fileTypes": ["js", "jsx", "mjs", "cjs"],
  "patterns": [
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#regex" },
    { "include": "#keywords" },
    { "include": "#functions" },
    { "include": "#numbers" },
    { "include": "#operators" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.js",
          "match": "//.*$"
        },
        {
          "name": "comment.block.js",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.js",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.js",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.js",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.js",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.template.js",
          "begin": "`",
          "end": "`",
          "patterns": [
            {
              "name": "meta.embedded.js",
              "begin": "\\$\\{",
              "end": "\\}"
            }
          ]
        }
      ]
    },
    "regex": {
      "patterns": [
        {
          "name": "string.regexp.js",
          "match": "(?<=[=(:,\\[!&|?]|^|return|case)\\s*/(?![/*])(?:[^/\\\\\\n]|\\\\.)+/[gimsuvy]*"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.js",
          "match": "\\b(if|else|switch|case|default|for|while|do|break|continue|return|throw|try|catch|finally|with|debugger|yield|await)\\b"
        },
        {
          "name": "keyword.declaration.js",
          "match": "\\b(var|let|const|function|class|extends|import|export|from|as|default|async|static|get|set)\\b"
        },
        {
          "name": "constant.language.js",
          "match": "\\b(true|false|null|undefined|NaN|Infinity|this|super|arguments)\\b"
        },
        {
          "name": "keyword.operator.js",
          "match": "\\b(new|delete|typeof|instanceof|in|of|void)\\b"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "entity.name.function.js",
          "match": "\\b([a-zA-Z_$][a-zA-Z0-9_$]*)\\s*(?=\\()"
        },
        {
          "name": "support.function.js",
          "match": "\\b(console|Math|JSON|Object|Array|String|Number|Boolean|Date|RegExp|Error|Promise|Map|Set|WeakMap|WeakSet|Symbol|Proxy|Reflect|parseInt|parseFloat|isNaN|isFinite|decodeURI|encodeURI|eval|alert|confirm|prompt|setTimeout|setInterval|clearTimeout|clearInterval|fetch|require)\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.js",
          "match": "\\b(0x[0-9a-fA-F]+|0o[0-7]+|0b[01]+|[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?n?)\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.js",
          "match": "[+\\-*/%=<>!&|^~?:]+|=>|\\.\\.\\.?"
        }
      ]
    }
  }
}
